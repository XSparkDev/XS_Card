{
  "cli": {
    "version": ">= 3.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "distribution": "internal"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "disabled": false,
        "cacheDefaultPaths": true,
        "paths": ["node_modules", ".expo", "ios/Podfile.lock"]
      },
      "env": {
        "NODE_ENV": "production"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "distribution": "store",
        "cache": {
          "disabled": false
        },
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
